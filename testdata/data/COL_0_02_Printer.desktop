[Desktop Entry]
X-SuSE-translate=true
OnlyShowIn=KDE;
Name=Printer
Actions=Print;List;
Exec=/opt/kde3/bin/startprint %u
Icon=printer1
Type=Application
X-KDE-SubstituteUID=false
renameOnDesktop=true

[Desktop Action List]
Exec=kjobviewer --show
Name=State for all printers
[Desktop Action Print]
Exec=kprinter %u
Name=Print files


